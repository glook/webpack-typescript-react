{
    "name": "webpack-typescript-react",
    "version": "1.0.5",
    "description": "Webpack 5 boilerplate with support of most common loaders and modules",
    "keywords": [
        "react",
        "typescript",
        "webpack",
        "webpack 5",
        "webpack boilerplate"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:glook/webpack-typescript-react.git"
    },
    "license": "MIT",
    "author": "Andrey Polyakov <andrey@polyakov.im>",
    "main": "webpack.config.babel.js",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
        "profile": "cross-env NODE_ENV=production webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
        "start": "cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config webpack.config.babel.js",
        "release": "npm version patch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "post-commit": "git update-index --again"
        }
    },
    "dependencies": {
        "classnames": "^2.3.1",
        "normalize.css": "^8.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/core": "~7.12.3",
        "@babel/plugin-proposal-class-properties": "~7.12.1",
        "@babel/plugin-proposal-export-default-from": "^7.12.1",
        "@babel/plugin-proposal-export-namespace-from": "~7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "~7.12.1",
        "@babel/plugin-proposal-throw-expressions": "~7.12.1",
        "@babel/plugin-syntax-dynamic-import": "~7.8.3",
        "@babel/plugin-transform-runtime": "~7.12.1",
        "@babel/preset-env": "~7.12.1",
        "@babel/preset-react": "~7.12.1",
        "@babel/register": "~7.12.1",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
        "@svgr/webpack": "^5.5.0",
        "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
        "@types/classnames": "^2.3.1",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "~4.7.0",
        "@typescript-eslint/parser": "~4.7.0",
        "autoprefixer": "^10.4.0",
        "babel-eslint": "~10.1.0",
        "babel-loader": "^8.2.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^9.1.0",
        "core-js": "~3.7.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "cssnano": "^5.0.10",
        "eslint": "~7.13.0",
        "eslint-config-airbnb-base": "~14.2.0",
        "eslint-config-airbnb-typescript": "~12.0.0",
        "eslint-config-prettier": "~6.15.0",
        "eslint-import-resolver-alias": "~1.1.2",
        "eslint-plugin-import": "~2.22.1",
        "eslint-plugin-jsx-a11y": "~6.4.1",
        "eslint-plugin-react": "~7.21.5",
        "eslint-plugin-react-hooks": "~4.2.0",
        "eslint-webpack-plugin": "^3.1.1",
        "fork-ts-checker-webpack-plugin": "^6.4.0",
        "html-loader": "^3.0.1",
        "html-webpack-plugin": "^5.5.0",
        "husky": "~4.3.0",
        "import-sort-style-module-and-prefix": "~0.1.3",
        "is-windows": "^1.0.2",
        "less": "~3.12.2",
        "less-loader": "^10.2.0",
        "lint-staged": "~10.5.0",
        "mini-css-extract-plugin": "^2.4.4",
        "path": "~0.12.7",
        "postcss": "^8.3.11",
        "postcss-loader": "^6.2.0",
        "prettier": "~2.1.2",
        "prettier-plugin-import-sort": "~0.0.6",
        "pretty-quick": "~3.1.0",
        "react-refresh": "^0.10.0",
        "regenerator-runtime": "~0.13.7",
        "resolve-url-loader": "^4.0.0",
        "sass": "~1.29.0",
        "sass-loader": "^12.3.0",
        "sass-resources-loader": "^2.2.4",
        "style-loader": "^3.3.1",
        "svg-url-loader": "^7.1.1",
        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.64.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.5.0",
        "webpack-merge": "^5.8.0"
    },
    "importSort": {
        ".ts, .tsx": {
            "style": "module-and-prefix",
            "parser": "typescript"
        },
        ".js, .jsx": {
            "style": "module-and-prefix",
            "parser": "babylon"
        }
    }
}
